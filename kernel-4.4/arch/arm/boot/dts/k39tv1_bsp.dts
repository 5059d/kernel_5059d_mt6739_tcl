&i2c1 {
/*  start sensor */
	cust_accel@0 {
		compatible = "mediatek,mc3413-p";
		//i2c_num	= <1>;
		direction = <6>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <0>;
		is_batch_supported = <0>;
	};
	cust_alsps@0 {
		compatible = "mediatek,stk3x1x";
		/*i2c_num = <1>;
		i2c_addr = <0x51 0 0 0>;*/
		polling_mode_ps = <0>;
		polling_mode_als = <1>;
		power_id = <0xffff>;
		power_vol = <0>;
		als_level = <0 328 861 1377 3125 7721 7767 12621 23062 28430 33274 47116 57694 57694 65535>;
		als_value = <0 133 304 502 1004 2005 3058 5005 8008 10010 12000 16000 20000 20000 20000 20000>;
		ps_threshold_high = <26>;
		ps_threshold_low = <21>;
		is_batch_supported_ps = <0>;
		is_batch_supported_als = <0>;
	};
	cust_accel@1 {
		compatible = "mediatek,bma253_new";
		//i2c_num	= <1>;
		direction = <4>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <0>;
		is_batch_supported = <0>;
       };

	cust_mag@0 {
		compatible			= "mediatek,akmd09918";
		//i2c_num				= <1>;
		/* i2c_addr			= <0x0C 0 0 0>; */
		direction			= <8>;
		power_id			= <0xffff>;
		power_vol			= <0>;
		/* is_batch_supported		= <0>; */
	};

	cust_gyro@0 {
		compatible			= "mediatek,itg1010";
		//i2c_num				= <1>;
		/* i2c_addr			= <0x68 0 0 0>; */
		direction			= <5>;
		power_id			= <0xffff>;
		power_vol			= <0>;
		firlen				= <0>;
		is_batch_supported		= <0>;
	};

	cust_baro@0 {
		compatible = "mediatek,bmp280new";
		i2c_num = <1>;
		/* i2c_addr = <0x77 0 0 0>; */
		direction = <0>;
		power_id = <0xffff>;
		power_vol = <0>;
		firlen = <32>;
		is_batch_supported = <0>;
	};
/* end sensor */
};
